  <config>
    <fields name="params" addfieldpath="/modules/mod_GI_glassymenu/params">
      <fieldset name="basic">
        <field name="update" type="offlajnupdatechecker" label="-"/>
        <field name="position" type="list" default="1" label="Menu position on the screen" description="In which position do you want to show the menu items?">
          <option value="1">Left</option>
          <option value="2">Right</option>
        </field>
        <field name="fixed" type="list" default="1" label="Menu fixed to the browser" description="Do you want to fix the menu to the browser or scrolling with the page?">
          <option value="1">Yes</option>
          <option value="2">No</option>
        </field>
        <field name="menutop" type="text" default="50" label="The menu top position" description="The menu top position (y-axis) from the (top of the browser/top of the site)"/>
        <field name="opacity" type="text" default="0.5" label="Opacity" description="The menu opacity when it is hidden"/>
        <field name="menuitemdistance" type="text" default="10" label="Distance between the menu items (px)" description="Distance between the menu items"/>
        <field name="theme" type="theme" label="Level" description=""/>
      </fieldset>
    </fields>
  </config>
  <params addPath="/modules/mod_GI_glassymenu/params">
    <param name="update" type="offlajnupdatechecker" label="-"/>
