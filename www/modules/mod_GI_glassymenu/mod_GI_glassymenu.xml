      <option value="2">No</option>
    </param>
    <param name="menutop" type="text" default="50" label="The menu top position" description="The menu top position (y-axis) from the (top of the browser/top of the site)"/>
    <param name="opacity" type="text" default="0.5" label="Opacity" description="The menu opacity when it is hidden"/>
    <param name="menuitemdistance" type="text" default="10" label="Distance between the menu items (px)" description="Distance between the menu items"/>
    <param name="theme" type="theme" label="Level" description=""/>
